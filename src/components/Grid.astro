---

const { items } = Astro.props;

---

<div id="grid">

<div id="bck"></div>

<main>
    {items.map((item,i) => (
        <img 
        loading="lazy"
            data-index = {i}
            data-description={item.content}
            src={'https://cms.computomanzia.link/storage/'+item.image}
        />
    ))}

</main>

</div>
